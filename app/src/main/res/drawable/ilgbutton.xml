<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="false">
		<shape>
			<gradient android:startColor="#D9D9D9" android:endColor="#C7C7C7"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#B3B3B3" />
			<corners android:radius="5dp" />
			<padding android:left="10dp" android:top="10dp"
				android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
	<item android:state_pressed="true">
		<shape>
			<gradient android:startColor="#FFFFFF" android:endColor="#D1E1EF"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#D9D9D9" />
			<corners android:radius="5dp" />
			<padding android:left="10dp" android:top="10dp"
				android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
	<item android:state_focused="true">
		<shape>
			<gradient android:startColor="#001F37" android:endColor="#000000"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#D9D9D9" />
			<corners android:radius="5dp" />
			<padding android:left="10dp" android:top="10dp"
				android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient android:startColor="#295273" android:endColor="#001F37"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#FFFFFF" />
			<corners android:radius="5dp" />
			<padding android:left="10dp" android:top="10dp"
				android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
</selector>